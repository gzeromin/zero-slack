{
	"compilerOptions": {
		"esModuleInterop": true,
		"sourceMap": true,
		"lib": ["ES2020", "DOM"],
		"jsx": "react",
		"module": "ESNext",
		"moduleResolution": "Node",
		"target": "ES5",
		"strict": true,
		"resolveJsonModule": true,
		"baseUrl": ".",
		"paths": {
			"@hooks/*": ["hooks/*"],
			"@components/*": ["components/*"],
			"@layouts/*": ["layouts/*"],
			"@pages/*": ["pages/*"],
			"@utils/*": ["utils/*"],
			"@typings/*": ["typings/*"]
		}
	}
}